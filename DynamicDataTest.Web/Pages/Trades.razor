@page "/trades"
@using DynamicDataTest.Web.Data.Trades
@inherits ReactiveUI.Blazor.ReactiveComponentBase<DynamicDataTest.Web.Data.Trades.TradeViewModel>
@inject TradeViewModel InjectedVm
<h3>Trades</h3>

@foreach (var trade in ViewModel.Data)
{
    <h3>Id: @trade.Id</h3>
    <h3>Amount: @trade.Amount</h3>
                             <br/>
}


@code 
{
    /// <inheritdoc />
    protected override void OnInitialized()
    {
        this.ViewModel = this.InjectedVm;
        base.OnInitialized();
    }


}